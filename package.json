  "version": "0.0.1",
  "main": "webpack-prod.config.js",
  "scripts": {
    "start": "webpack-dev-server --config webpack-dev.config.js --content-base src --inline --hot",
    "build": "webpack --config webpack-prod.config.js --content-base src",
    "postinstall": "node fixModules.js",
    "bkc-mine": "node blockchain.js mine",
    "bkc-init": "node blockchain.js init",
    "bkc-accounts": "node blockchain.js accounts",
    "bkc-contracts": "node blockchain.js contracts",
    "bkc-clean": "rm -R blockchain/geth",
    "geth-console": "go-ethereum/build/bin/geth --networkid 123456 --datadir=blockchain --rpc --rpcaddr=localhost --rpcport 8545 --rpccorsdomain=* --nodiscover --maxpeers=0 console",
    "geth-console-example": "go-ethereum/build/bin/geth --networkid 123456 --datadir=example/blockchain --rpc --rpcaddr=localhost --rpcport 8545 --rpccorsdomain=* --nodiscover --maxpeers=0 console"
  },
